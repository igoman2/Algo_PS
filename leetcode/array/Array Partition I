// https://leetcode.com/problems/array-partition-i
/**
 * @param {number[]} nums
 * @return {number}
 */
// 절차형
var arrayPairSum = function(nums) {
    nums = nums.sort((a,b) => {
        return a-b
    })
    let sum = 0;
    for(let i=1; i<nums.length; i+=2){
        sum = sum + Math.min(nums[i-1], nums[i])
    }
    return sum
};


// 함수형

/**
 * @param {number} x
 * @return {number}
 */
var mySqrt = function(x) {
    let count = 0;
    while(true){
        if(count*count > x){
            return Math.floor(count) - 1
        }
        else if(count*count == x){
            return count
        }
        else{
            count++
        }
    }
};

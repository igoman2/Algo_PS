/**
 * Definition for singly-linked list.
 * function ListNode(val) {
 *     this.val = val;
 *     this.next = null;
 * }
 */

/**
 * @param {ListNode} head
 * @return {boolean}
 */
 
// ν¬ ν¬μΈν„°  
var hasCycle = function(head) {
    let fast = head
    let slow = head
    while(1){
        if(!fast || !fast.next){
            return false
        }
        fast = fast.next.next
        slow = slow.next
        if(fast == slow) return true
    }
};
